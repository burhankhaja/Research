        </mxCell>
        <mxCell id="3ofdXOLbKWFfFdMxITbS-78" value="liquidator pays $5 usdc&#xa;liquidator == 5 + 0.5 --&gt; $5.5 eth&#xa;user debt == $85 - $5 = $80 usdc&#xa;user collateral == $100 - $5.5 = $94.5&#xa;LTV === $80/$94.5 ==&gt; 84% " style="text;whiteSpace=wrap;fontStyle=2" vertex="1" parent="1">
          <mxGeometry x="6010" y="800" width="210" height="90" as="geometry" />
        </mxCell>
