        </mxCell>
        <mxCell id="3ofdXOLbKWFfFdMxITbS-113" value="(Total Collateral Value * Weighted Average Liquidation Threshold) / Total Borrow Value" style="text;whiteSpace=wrap;fontSize=18;fillColor=#fff2cc;strokeColor=none;align=center;verticalAlign=middle;" vertex="1" parent="1">
